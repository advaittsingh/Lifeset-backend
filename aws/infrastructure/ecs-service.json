{
  "serviceName": "lifeset-backend-service",
  "cluster": "REPLACE_WITH_CLUSTER_NAME",
  "taskDefinition": "REPLACE_WITH_TASK_DEFINITION_ARN",
  "desiredCount": 2,
  "launchType": "FARGATE",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "REPLACE_WITH_PRIVATE_SUBNET_1",
        "REPLACE_WITH_PRIVATE_SUBNET_2"
      ],
      "securityGroups": [
        "REPLACE_WITH_ECS_SECURITY_GROUP_ID"
      ],
      "assignPublicIp": "DISABLED"
    }
  },
  "loadBalancers": [
    {
      "targetGroupArn": "REPLACE_WITH_TARGET_GROUP_ARN",
      "containerName": "lifeset-backend",
      "containerPort": 3000
    }
  ],
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 100,
    "deploymentCircuitBreaker": {
      "enable": true,
      "rollback": true
    }
  },
  "healthCheckGracePeriodSeconds": 60,
  "enableExecuteCommand": true
}

